{"ast":null,"code":"import { Avatar, Lock } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n\n  data() {\n    return {\n      Avatar,\n      Lock,\n      password2: '',\n      dis: true,\n      form: {},\n      rules: {\n        id: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    //判断两次密码是否一致\n    test() {\n      if (this.form.password != this.password2) {\n        this.$message({\n          type: \"error\",\n          message: \"两次密码不同\"\n        });\n        this.dis = true;\n      } else {\n        this.dis = false;\n      }\n    },\n\n    register() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (this.form.password == this.password2) {\n            if (this.form.id.length > 3) {\n              if (this.form.name != null) {\n                // 登录\n                request.post(\"/user\", this.form).then(res => {\n                  console.log(res);\n\n                  if (res.code === '0') {\n                    this.$message({\n                      type: \"success\",\n                      message: \"注册成功\"\n                    });\n                    this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\n                  } else {\n                    this.$message({\n                      type: \"warning\",\n                      message: res.msg\n                    });\n                  }\n                });\n              } else {\n                this.$message({\n                  type: \"error\",\n                  message: \"昵称不合法\"\n                });\n              }\n            } else {\n              this.$message({\n                type: \"error\",\n                message: \"用户名不合法\"\n              });\n            }\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"两次密码不同\"\n            });\n          }\n        }\n      });\n    }\n\n  }\n}; // 注意，登录后sessionShort存储情况如下：\n// key=currentId,value=学号/工号\n// key=currentName,value=姓名\n// key=currentDep,value=院系号，管理员为0\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串","map":{"version":3,"mappings":"AAmCA,SAAQA,MAAR,EAAgBC,IAAhB,QAA2B,yBAA3B;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLJ,YADK;AAELC,UAFK;AAGLI,eAAS,EAAE,EAHN;AAILC,SAAG,EAAE,IAJA;AAKLC,UAAI,EAAE,EALD;AAMLC,WAAK,EAAE;AACLC,UAAE,EAAE,CACF;AAACC,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,QAA1B;AAAoCC,iBAAO,EAAE;AAA7C,SADE,CADC;AAILC,gBAAQ,EAAE,CACR;AAACH,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,OAA1B;AAAmCC,iBAAO,EAAE;AAA5C,SADQ;AAJL;AANF,KAAP;AAeD,GAlBY;;AAmBbE,SAAO,EAAE;AACP;AACAC,QAAI,GAAG;AACL,UAAI,KAAKR,IAAL,CAAUM,QAAV,IAAsB,KAAKR,SAA/B,EAA0C;AACxC,aAAKW,QAAL,CAAc;AACZC,cAAI,EAAE,OADM;AAEZN,iBAAO,EAAE;AAFG,SAAd;AAIA,aAAKL,GAAL,GAAW,IAAX;AACF,OANA,MAMO;AACL,aAAKA,GAAL,GAAW,KAAX;AACF;AACD,KAZM;;AAaPY,YAAQ,GAAG;AACT,WAAKC,KAAL,CAAW,MAAX,EAAmBC,QAAnB,CAA6BC,KAAD,IAAW;AACrC,YAAIA,KAAJ,EAAW;AACT,cAAI,KAAKd,IAAL,CAAUM,QAAV,IAAsB,KAAKR,SAA/B,EAA0C;AACxC,gBAAI,KAAKE,IAAL,CAAUE,EAAV,CAAaa,MAAb,GAAsB,CAA1B,EAA6B;AAC3B,kBAAG,KAAKf,IAAL,CAAUJ,IAAV,IAAkB,IAArB,EAA2B;AAAC;AAC1BD,uBAAO,CAACqB,IAAR,CAAa,OAAb,EAAsB,KAAKhB,IAA3B,EAAiCiB,IAAjC,CAAsCC,GAAE,IAAK;AAC3CC,yBAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,sBAAIA,GAAG,CAACG,IAAJ,KAAa,GAAjB,EAAsB;AACpB,yBAAKZ,QAAL,CAAc;AACZC,0BAAI,EAAE,SADM;AAEZN,6BAAO,EAAE;AAFG,qBAAd;AAKA,yBAAKkB,OAAL,CAAaC,IAAb,CAAkB,OAAlB,EANoB,CAMQ;AAC9B,mBAPA,MAOO;AACL,yBAAKd,QAAL,CAAc;AACZC,0BAAI,EAAE,SADM;AAEZN,6BAAO,EAAEc,GAAG,CAACM;AAFD,qBAAd;AAIF;AACD,iBAfD;AAgBD,eAjBD,MAiBK;AACH,qBAAKf,QAAL,CAAc;AACZC,sBAAI,EAAE,OADM;AAEZN,yBAAO,EAAE;AAFG,iBAAd;AAIF;AACF,aAxBA,MAwBO;AACL,mBAAKK,QAAL,CAAc;AACZC,oBAAI,EAAE,OADM;AAEZN,uBAAO,EAAE;AAFG,eAAd;AAIF;AACD,WA/BD,MA+BK;AACH,iBAAKK,QAAL,CAAc;AACZC,kBAAI,EAAE,OADM;AAEZN,qBAAO,EAAE;AAFG,aAAd;AAIF;AACF;AACD,OAxCD;AAyCF;;AAvDO;AAnBI,CAAf,C,CA6EA;AACA;AACA;AACA;AACA","names":["Avatar","Lock","request","name","data","password2","dis","form","rules","id","required","message","trigger","password","methods","test","$message","type","register","$refs","validate","valid","length","post","then","res","console","log","code","$router","push","msg"],"sourceRoot":"","sources":["E:\\code\\springboot+vue\\class_choose_book\\vue\\src\\views\\register.vue"],"sourcesContent":["<template>\r\n  <div class=\"index-bg\"></div>\r\n  <div style=\"overflow: hidden;\">\r\n    <el-form ref=\"form\" :model=\"form\" class=\"login\" :rules=\"rules\">\r\n      <div style=\"color: darkslategrey; font-size: 30px; text-align: center; padding: 40px 0;\">注册</div>\r\n      <el-form-item prop=\"id\">\r\n        <el-input :prefix-icon=\"Avatar\" placeholder=\"请输入ID/用户名\" v-model=\"form.id\"/>\r\n      </el-form-item>\r\n      <el-form-item prop=\"password\">\r\n        <el-input :prefix-icon=\"Lock\" placeholder=\"请输入密码\" v-model=\"form.password\" show-password/>\r\n      </el-form-item>\r\n      <el-form-item prop=\"password\">\r\n        <el-input :prefix-icon=\"Lock\" placeholder=\"请再次输入密码\" v-model=\"password2\" show-password @blur=\"test\"/>\r\n      </el-form-item>\r\n      <el-form-item prop=\"name\">\r\n        <el-input :prefix-icon=\"Avatar\" placeholder=\"请输入昵称\" v-model=\"form.name\" />\r\n      </el-form-item>\r\n      <el-form-item   prop=\"sex\">\r\n        <div>性别：</div>\r\n          <el-radio-group v-model=\"form.sex\" >\r\n            <el-radio label=\"男\" size=\"large\">男</el-radio>\r\n            <el-radio label=\"女\" size=\"large\">女</el-radio>\r\n            <el-radio label=\"未知\" size=\"large\">未知</el-radio>\r\n          </el-radio-group>\r\n      </el-form-item>\r\n      <!--      <div style=\"text-align: center;\">-->\r\n      <!--        <el-button style=\"width: 80%;\" type=\"info\" @click=\"login\">登 录</el-button>-->\r\n      <!--        <div style=\"height: 50px; background-color: deepskyblue; width: 80%; margin: 0 auto;\"></div>-->\r\n      <!--      </div>-->\r\n      <el-button :disabled=\"dis\" style=\"width: 100%;\" type=\"info\" @click=\"register\">注册</el-button>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {Avatar, Lock} from '@element-plus/icons-vue';\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      Avatar,\r\n      Lock,\r\n      password2: '',\r\n      dis: true,\r\n      form: {},\r\n      rules: {\r\n        id: [\r\n          {required: true, message: '请输入用户名', trigger: 'blur'},\r\n        ],\r\n        password: [\r\n          {required: true, message: '请输入密码', trigger: 'blur'},\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    //判断两次密码是否一致\r\n    test() {\r\n      if (this.form.password != this.password2) {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"两次密码不同\"\r\n        })\r\n        this.dis = true\r\n      } else {\r\n        this.dis = false\r\n      }\r\n    },\r\n    register() {\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          if (this.form.password == this.password2) {\r\n            if (this.form.id.length > 3) {\r\n              if(this.form.name != null) {// 登录\r\n                request.post(\"/user\", this.form).then(res => {\r\n                  console.log(res);\r\n                  if (res.code === '0') {\r\n                    this.$message({\r\n                      type: \"success\",\r\n                      message: \"注册成功\"\r\n                    })\r\n\r\n                    this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\r\n                  } else {\r\n                    this.$message({\r\n                      type: \"warning\",\r\n                      message: res.msg\r\n                    })\r\n                  }\r\n                })\r\n              }else{\r\n                this.$message({\r\n                  type: \"error\",\r\n                  message: \"昵称不合法\"\r\n                })\r\n              }\r\n            } else {\r\n              this.$message({\r\n                type: \"error\",\r\n                message: \"用户名不合法\"\r\n              })\r\n            }\r\n          }else{\r\n            this.$message({\r\n              type: \"error\",\r\n              message: \"两次密码不同\"\r\n            })\r\n          }\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n// 注意，登录后sessionShort存储情况如下：\r\n// key=currentId,value=学号/工号\r\n// key=currentName,value=姓名\r\n// key=currentDep,value=院系号，管理员为0\r\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串\r\n</script>\r\n\r\n<style>\r\n.login {\r\n  background-color: wheat;\r\n  width: 30%;\r\n  height: 380px;\r\n  opacity: 0.8;\r\n  border-radius: 30px;\r\n  margin: 220px 300px 180px 500px;\r\n\r\n\r\n  /* 为其整体设置接近透明的效果*/\r\n  background-color: rgba(255, 255, 255, 1);\r\n  /* 设置box-shadow使其有立体感 */\r\n  box-shadow: 5px 5px 0 0 rgba(0, 0, 0, 1);\r\n\r\n  padding: 0 50px;\r\n}\r\n\r\n* {\r\n  margin: 0;\r\n}\r\n\r\n/*背景图*/\r\n.index-bg {\r\n  width: 100%;\r\n  height: 522px;\r\n  background: url(../assets/home-banner.jpg) no-repeat;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -1;\r\n  background-size: 100% 522px;\r\n  min-width: 1000px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}