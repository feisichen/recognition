{"ast":null,"code":"import { Avatar, Lock } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n\n  data() {\n    return {\n      Avatar,\n      Lock,\n      form: {},\n      rules: {\n        id: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    back() {\n      // if (window.history.length <= 1) {\n      //   this.$router.push({path:'/'})\n      //   return false\n      // } else {\n      //   this.$router.go(-1)\n      // }\n      this.$router.push({\n        path: '/home'\n      });\n    },\n\n    login() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (this.form.id.length == 8) {\n            // 登录\n            request.post(\"/user/login\", this.form).then(res => {\n              if (res.code === '0') {\n                this.$message({\n                  type: \"success\",\n                  message: \"登录成功\"\n                }); // request.get(\"/department/forHeader\", {\n                //   params: {\n                //     dep: res.data.department\n                //   }\n                // }).then(res2 => {\n                //   sessionStorage.setItem(\"currentDepName\", \"你好，\" + res2.data[0].departmentName + \"，\")\n                // })\n\n                sessionStorage.setItem(\"currentId\", res.data.id);\n                sessionStorage.setItem(\"currentName\", res.data.name);\n                this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\n              } else {\n                this.$message({\n                  type: \"error\",\n                  message: res.msg\n                });\n              }\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"用户名不合法\"\n            });\n          }\n        }\n      });\n    }\n\n  }\n}; // 注意，登录后sessionShort存储情况如下：\n// key=currentId,value=学号/工号\n// key=currentName,value=姓名\n// key=currentDep,value=院系号，管理员为0\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串","map":{"version":3,"mappings":"AA0BA,SAAQA,MAAR,EAAgBC,IAAhB,QAA2B,yBAA3B;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLJ,YADK;AAELC,UAFK;AAGLI,UAAI,EAAE,EAHD;AAILC,WAAK,EAAE;AACLC,UAAE,EAAE,CACF;AAACC,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,QAA1B;AAAoCC,iBAAO,EAAE;AAA7C,SADE,CADC;AAILC,gBAAQ,EAAE,CACR;AAACH,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,OAA1B;AAAmCC,iBAAO,EAAE;AAA5C,SADQ;AAJL;AAJF,KAAP;AAaD,GAhBY;;AAiBbE,SAAO,EAAE;AACPC,QAAI,GAAE;AACJ;AACA;AACA;AACA;AACA;AACA;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAACC,YAAI,EAAC;AAAN,OAAlB;AACD,KATM;;AAUPC,SAAK,GAAG;AACN,WAAKC,KAAL,CAAW,MAAX,EAAmBC,QAAnB,CAA6BC,KAAD,IAAW;AACrC,YAAIA,KAAJ,EAAW;AACT,cAAI,KAAKf,IAAL,CAAUE,EAAV,CAAac,MAAb,IAAuB,CAA3B,EAA8B;AAAE;AAC9BnB,mBAAO,CAACoB,IAAR,CAAa,aAAb,EAA4B,KAAKjB,IAAjC,EAAuCkB,IAAvC,CAA4CC,GAAE,IAAK;AACjD,kBAAIA,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;AACpB,qBAAKC,QAAL,CAAc;AACZC,sBAAI,EAAE,SADM;AAEZlB,yBAAO,EAAE;AAFG,iBAAd,EADoB,CAKpB;AACA;AACA;AACA;AACA;AACA;AACA;;AACAmB,8BAAc,CAACC,OAAf,CAAuB,WAAvB,EAAoCL,GAAG,CAACpB,IAAJ,CAASG,EAA7C;AACAqB,8BAAc,CAACC,OAAf,CAAuB,aAAvB,EAAsCL,GAAG,CAACpB,IAAJ,CAASD,IAA/C;AAEA,qBAAKW,OAAL,CAAaC,IAAb,CAAkB,OAAlB,EAfoB,CAeQ;AAC9B,eAhBA,MAgBO;AACL,qBAAKW,QAAL,CAAc;AACZC,sBAAI,EAAE,OADM;AAEZlB,yBAAO,EAAEe,GAAG,CAACM;AAFD,iBAAd;AAIF;AACD,aAvBD;AAwBF,WAzBA,MAyBO;AACL,iBAAKJ,QAAL,CAAc;AACZC,kBAAI,EAAE,OADM;AAEZlB,qBAAO,EAAE;AAFG,aAAd;AAIF;AACF;AACD,OAlCD;AAmCF;;AA9CO;AAjBI,CAAf,C,CAkEA;AACA;AACA;AACA;AACA","names":["Avatar","Lock","request","name","data","form","rules","id","required","message","trigger","password","methods","back","$router","push","path","login","$refs","validate","valid","length","post","then","res","code","$message","type","sessionStorage","setItem","msg"],"sourceRoot":"","sources":["E:\\code\\springboot+vue\\class_choose_book\\vue\\src\\views\\Login.vue"],"sourcesContent":["<template>\n  <div class=\"index-bg\"></div>\n  <el-button @click=\"back\"\n             style=\"margin:13px 100px 10px 15px;color: #ebeef5\"\n          type=\"text\"\n          font-color=\"#fff\"\n  >返回</el-button>\n  <div style=\"overflow: hidden;\">\n    <el-form ref=\"form\" :model=\"form\" class=\"login\" :rules=\"rules\">\n      <div style=\"color: darkslategrey; font-size: 30px; text-align: center; padding: 40px 0;\">登陆</div>\n      <el-form-item prop=\"id\">\n        <el-input :prefix-icon=\"Avatar\" placeholder=\"请输入ID/用户名\" v-model=\"form.id\"/>\n      </el-form-item>\n      <el-form-item prop=\"password\">\n        <el-input :prefix-icon=\"Lock\" placeholder=\"请输入密码\" v-model=\"form.password\" show-password/>\n      </el-form-item>\n      <!--      <div style=\"text-align: center;\">-->\n      <!--        <el-button style=\"width: 80%;\" type=\"info\" @click=\"login\">登 录</el-button>-->\n      <!--        <div style=\"height: 50px; background-color: deepskyblue; width: 80%; margin: 0 auto;\"></div>-->\n      <!--      </div>-->\n      <el-button style=\"width: 100%;\" type=\"info\" @click=\"login\">登 录</el-button>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport {Avatar, Lock} from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      Avatar,\n      Lock,\n      form: {},\n      rules: {\n        id: [\n          {required: true, message: '请输入用户名', trigger: 'blur'},\n        ],\n        password: [\n          {required: true, message: '请输入密码', trigger: 'blur'},\n        ]\n      }\n    }\n  },\n  methods: {\n    back(){\n      // if (window.history.length <= 1) {\n      //   this.$router.push({path:'/'})\n      //   return false\n      // } else {\n      //   this.$router.go(-1)\n      // }\n      this.$router.push({path:'/home'})\n    },\n    login() {\n      this.$refs['form'].validate((valid) => {\n        if (valid) {\n          if (this.form.id.length == 8) { // 登录\n            request.post(\"/user/login\", this.form).then(res => {\n              if (res.code === '0') {\n                this.$message({\n                  type: \"success\",\n                  message: \"登录成功\"\n                })\n                // request.get(\"/department/forHeader\", {\n                //   params: {\n                //     dep: res.data.department\n                //   }\n                // }).then(res2 => {\n                //   sessionStorage.setItem(\"currentDepName\", \"你好，\" + res2.data[0].departmentName + \"，\")\n                // })\n                sessionStorage.setItem(\"currentId\", res.data.id)\n                sessionStorage.setItem(\"currentName\", res.data.name)\n\n                this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\n              } else {\n                this.$message({\n                  type: \"error\",\n                  message: res.msg\n                })\n              }\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"用户名不合法\"\n            })\n          }\n        }\n      })\n    }\n  }\n}\n// 注意，登录后sessionShort存储情况如下：\n// key=currentId,value=学号/工号\n// key=currentName,value=姓名\n// key=currentDep,value=院系号，管理员为0\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串\n</script>\n\n<style>\n.login {\n  background-color: wheat;\n  width: 30%;\n  height: 300px;\n  opacity: 0.8;\n  border-radius: 30px;\n  margin: 220px 300px 220px 500px;\n\n\n  /* 为其整体设置接近透明的效果*/\n  background-color: rgba(255, 255, 255, 1);\n  /* 设置box-shadow使其有立体感 */\n  box-shadow: 5px 5px 0 0 rgba(0, 0, 0, 1);\n\n  padding: 0 50px;\n}\n\n* {\n  margin: 0;\n}\n\n/*背景图*/\n.index-bg {\n  width: 100%;\n  height: 522px;\n  background: url(../assets/home-banner.jpg) no-repeat;\n  position: absolute;\n  top: 0;\n  left: 0;\n  z-index: -1;\n  background-size: 100% 522px;\n  min-width: 1000px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}