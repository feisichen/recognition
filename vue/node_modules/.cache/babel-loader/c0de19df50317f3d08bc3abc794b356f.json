{"ast":null,"code":"import { Avatar, Lock } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n\n  data() {\n    return {\n      Avatar,\n      Lock,\n      form: {},\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        passwd: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    back() {\n      // if (window.history.length <= 1) {\n      //   this.$router.push({path:'/'})\n      //   return false\n      // } else {\n      //   this.$router.go(-1)\n      // }\n      this.$router.push({\n        path: '/home'\n      });\n    },\n\n    login() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (this.form.name.length >= 3) {\n            // 登录\n            request.post(\"/user/login\", this.form).then(res => {\n              if (res.code === '0') {\n                this.$message({\n                  type: \"success\",\n                  message: \"登录成功\"\n                }); // request.get(\"/department/forHeader\", {\n                //   params: {\n                //     dep: res.data.department\n                //   }\n                // }).then(res2 => {\n                //   sessionStorage.setItem(\"currentDepName\", \"你好，\" + res2.data[0].departmentName + \"，\")\n                // })\n\n                sessionStorage.setItem(\"currentId\", res.data.id);\n                sessionStorage.setItem(\"currentName\", res.data.name);\n                sessionStorage.setItem(\"flag\", 'false');\n                this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\n              } else {\n                this.$message({\n                  type: \"error\",\n                  message: res.msg\n                });\n              }\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"用户名不合法\"\n            });\n          }\n        }\n      });\n    }\n\n  }\n}; // 注意，登录后sessionShort存储情况如下：\n// key=currentId,value=学号/工号\n// key=currentName,value=姓名\n// key=currentDep,value=院系号，管理员为0\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串","map":{"version":3,"mappings":"AACA,SAAQA,MAAR,EAAgBC,IAAhB,QAA2B,yBAA3B;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,EAAAA,IAAI,EAAE,OADO;;AAEbC,EAAAA,IAAI,GAAG;AACL,WAAO;AACLJ,MAAAA,MADK;AAELC,MAAAA,IAFK;AAGLI,MAAAA,IAAI,EAAE,EAHD;AAILC,MAAAA,KAAK,EAAE;AACLH,QAAAA,IAAI,EAAE,CACJ;AAACI,UAAAA,QAAQ,EAAE,IAAX;AAAiBC,UAAAA,OAAO,EAAE,QAA1B;AAAoCC,UAAAA,OAAO,EAAE;AAA7C,SADI,CADD;AAILC,QAAAA,MAAM,EAAE,CACN;AAACH,UAAAA,QAAQ,EAAE,IAAX;AAAiBC,UAAAA,OAAO,EAAE,OAA1B;AAAmCC,UAAAA,OAAO,EAAE;AAA5C,SADM;AAJH;AAJF,KAAP;AAaD,GAhBY;;AAiBbE,EAAAA,OAAO,EAAE;AACPC,IAAAA,IAAI,GAAE;AACJ;AACA;AACA;AACA;AACA;AACA;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAACC,QAAAA,IAAI,EAAC;AAAN,OAAlB;AACD,KATM;;AAUPC,IAAAA,KAAK,GAAG;AACN,WAAKC,KAAL,CAAW,MAAX,EAAmBC,QAAnB,CAA6BC,KAAD,IAAW;AACrC,YAAIA,KAAJ,EAAW;AACT,cAAI,KAAKd,IAAL,CAAUF,IAAV,CAAeiB,MAAf,IAAyB,CAA7B,EAAgC;AAAE;AAChClB,YAAAA,OAAO,CAACmB,IAAR,CAAa,aAAb,EAA4B,KAAKhB,IAAjC,EAAuCiB,IAAvC,CAA4CC,GAAG,IAAI;AACjD,kBAAIA,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;AACpB,qBAAKC,QAAL,CAAc;AACZC,kBAAAA,IAAI,EAAE,SADM;AAEZlB,kBAAAA,OAAO,EAAE;AAFG,iBAAd,EADoB,CAKpB;AACA;AACA;AACA;AACA;AACA;AACA;;AACAmB,gBAAAA,cAAc,CAACC,OAAf,CAAuB,WAAvB,EAAoCL,GAAG,CAACnB,IAAJ,CAASyB,EAA7C;AACAF,gBAAAA,cAAc,CAACC,OAAf,CAAuB,aAAvB,EAAsCL,GAAG,CAACnB,IAAJ,CAASD,IAA/C;AACAwB,gBAAAA,cAAc,CAACC,OAAf,CAAuB,MAAvB,EAA+B,OAA/B;AAGA,qBAAKf,OAAL,CAAaC,IAAb,CAAkB,OAAlB,EAjBoB,CAiBQ;AAC7B,eAlBD,MAkBO;AACL,qBAAKW,QAAL,CAAc;AACZC,kBAAAA,IAAI,EAAE,OADM;AAEZlB,kBAAAA,OAAO,EAAEe,GAAG,CAACO;AAFD,iBAAd;AAID;AACF,aAzBD;AA0BD,WA3BD,MA2BO;AACL,iBAAKL,QAAL,CAAc;AACZC,cAAAA,IAAI,EAAE,OADM;AAEZlB,cAAAA,OAAO,EAAE;AAFG,aAAd;AAID;AACF;AACF,OApCD;AAqCD;;AAhDM;AAjBI,CAAf,C,CAoEA;AACA;AACA;AACA;AACA","names":["Avatar","Lock","r