{"ast":null,"code":"import { Avatar, Lock } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n\n  data() {\n    return {\n      Avatar,\n      Lock,\n      password2: '',\n      dis: true,\n      form: {},\n      rules: {\n        id: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    back() {\n      // if (window.history.length <= 1) {\n      //   this.$router.push({path:'/'})\n      //   return false\n      // } else {\n      //   this.$router.go(-1)\n      // }\n      this.$router.push({\n        path: '/home'\n      });\n    },\n\n    //判断两次密码是否一致\n    test() {\n      if (this.form.password != this.password2) {\n        this.$message({\n          type: \"error\",\n          message: \"两次密码不同\"\n        });\n        this.dis = true;\n      } else {\n        this.dis = false;\n      }\n    },\n\n    register() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (this.form.password == this.password2) {\n            if (this.form.id.length > 3) {\n              if (this.form.name != null) {\n                // 登录\n                request.post(\"/user\", this.form).then(res => {\n                  console.log(res);\n\n                  if (res.code === '0') {\n                    this.$message({\n                      type: \"success\",\n                      message: \"注册成功\"\n                    });\n                    this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\n                  } else {\n                    this.$message({\n                      type: \"warning\",\n                      message: res.msg\n                    });\n                  }\n                });\n              } else {\n                this.$message({\n                  type: \"error\",\n                  message: \"昵称不合法\"\n                });\n              }\n            } else {\n              this.$message({\n                type: \"error\",\n                message: \"用户名不合法\"\n              });\n            }\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"两次密码不同\"\n            });\n          }\n        }\n      });\n    }\n\n  }\n}; // 注意，登录后sessionShort存储情况如下：\n// key=currentId,value=学号/工号\n// key=currentName,value=姓名\n// key=currentDep,value=院系号，管理员为0\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串","map":{"version":3,"mappings":"AACA,SAAQA,MAAR,EAAgBC,IAAhB,QAA2B,yBAA3B;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,EAAAA,IAAI,EAAE,OADO;;AAEbC,EAAAA,IAAI,GAAG;AACL,WAAO;AACLJ,MAAAA,MADK;AAELC,MAAAA,IAFK;AAGLI,MAAAA,SAAS,EAAE,EAHN;AAILC,MAAAA,GAAG,EAAE,IAJA;AAKLC,MAAAA,IAAI,EAAE,EALD;AAMLC,MAAAA,KAAK,EAAE;AACLC,QAAAA,EAAE,EAAE,CACF;AAACC,UAAAA,QAAQ,EAAE,IAAX;AAAiBC,UAAAA,OAAO,EAAE,QAA1B;AAAoCC,UAAAA,OAAO,EAAE;AAA7C,SADE,CADC;AAILC,QAAAA,QAAQ,EAAE,CACR;AAACH,UAAAA,QAAQ,EAAE,IAAX;AAAiBC,UAAAA,OAAO,EAAE,OAA1B;AAAmCC,UAAAA,OAAO,EAAE;AAA5C,SADQ;AAJL;AANF,KAAP;AAeD,GAlBY;;AAmBbE,EAAAA,OAAO,EAAE;AACPC,IAAAA,IAAI,GAAE;AACJ;AACA;AACA;AACA;AACA;AACA;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAACC,QAAAA,IAAI,EAAC;AAAN,OAAlB;AACD,KATM;;AAUP;AACAC,IAAAA,IAAI,GAAG;AACL,UAAI,KAAKZ,IAAL,CAAUM,QAAV,IAAsB,KAAKR,SAA/B,EAA0C;AACxC,aAAKe,QAAL,CAAc;AACZC,UAAAA,IAAI,EAAE,OADM;AAEZV,UAAAA,OAAO,EAAE;AAFG,SAAd;AAIA,aAAKL,GAAL,GAAW,IAAX;AACD,OAND,MAMO;AACL,aAAKA,GAAL,GAAW,KAAX;AACD;AACF,KArBM;;AAsBPgB,IAAAA,QAAQ,GAAG;AACT,WAAKC,KAAL,CAAW,MAAX,EAAmBC,QAAnB,CAA6BC,KAAD,IAAW;AACrC,YAAIA,KAAJ,EAAW;AACT,cAAI,KAAKlB,IAAL,CAAUM,QAAV,IAAsB,KAAKR,SAA/B,EAA0C;AACxC,gBAAI,KAAKE,IAAL,CAAUE,EAAV,CAAaiB,MAAb,GAAsB,CAA1B,EAA6B;AAC3B,kBAAG,KAAKnB,IAAL,CAAUJ,IAAV,IAAkB,IAArB,EAA2B;AAAC;AAC1BD,gBAAAA,OAAO,CAACyB,IAAR,CAAa,OAAb,EAAsB,KAAKpB,IAA3B,EAAiCqB,IAAjC,CAAsCC,GAAG,IAAI;AAC3CC,kBAAAA,OAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,s