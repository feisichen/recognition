{"ast":null,"code":"import { Avatar, Lock } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n\n  data() {\n    return {\n      Avatar,\n      Lock,\n      password2: '',\n      form: {},\n      rules: {\n        id: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    //判断两次密码是否一致\n    test() {\n      console.log(this.form.password);\n      console.log(this.password2);\n    },\n\n    login() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (this.form.id.length == 8) {\n            // 登录\n            request.post(\"/student/login\", this.form).then(res => {\n              if (res.code === '0') {\n                this.$message({\n                  type: \"success\",\n                  message: \"登录成功\"\n                });\n                request.get(\"/department/forHeader\", {\n                  params: {\n                    dep: res.data.department\n                  }\n                }).then(res2 => {\n                  sessionStorage.setItem(\"currentDepName\", \"你好，\" + res2.data[0].departmentName + \"，\");\n                });\n                sessionStorage.setItem(\"currentId\", res.data.id);\n                sessionStorage.setItem(\"currentName\", res.data.name);\n                sessionStorage.setItem(\"currentDep\", res.data.department);\n                this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\n              } else {\n                this.$message({\n                  type: \"error\",\n                  message: res.msg\n                });\n              }\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"用户名不合法\"\n            });\n          }\n        }\n      });\n    }\n\n  }\n}; // 注意，登录后sessionShort存储情况如下：\n// key=currentId,value=学号/工号\n// key=currentName,value=姓名\n// key=currentDep,value=院系号，管理员为0\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串","map":{"version":3,"mappings":"AAwBA,SAAQA,MAAR,EAAgBC,IAAhB,QAA2B,yBAA3B;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLJ,YADK;AAELC,UAFK;AAGLI,eAAS,EAAC,EAHL;AAILC,UAAI,EAAE,EAJD;AAKLC,WAAK,EAAE;AACLC,UAAE,EAAE,CACF;AAACC,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,QAA1B;AAAoCC,iBAAO,EAAE;AAA7C,SADE,CADC;AAILC,gBAAQ,EAAE,CACR;AAACH,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,OAA1B;AAAmCC,iBAAO,EAAE;AAA5C,SADQ;AAJL;AALF,KAAP;AAcD,GAjBY;;AAkBbE,SAAO,EAAE;AACP;AACAC,QAAI,GAAE;AAEJC,aAAO,CAACC,GAAR,CAAY,KAAKV,IAAL,CAAUM,QAAtB;AACAG,aAAO,CAACC,GAAR,CAAY,KAAKX,SAAjB;AACD,KANM;;AAOPY,SAAK,GAAG;AACN,WAAKC,KAAL,CAAW,MAAX,EAAmBC,QAAnB,CAA6BC,KAAD,IAAW;AACrC,YAAIA,KAAJ,EAAW;AACT,cAAI,KAAKd,IAAL,CAAUE,EAAV,CAAaa,MAAb,IAAuB,CAA3B,EAA8B;AAAE;AAC9BnB,mBAAO,CAACoB,IAAR,CAAa,gBAAb,EAA+B,KAAKhB,IAApC,EAA0CiB,IAA1C,CAA+CC,GAAE,IAAK;AACpD,kBAAIA,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;AACpB,qBAAKC,QAAL,CAAc;AACZC,sBAAI,EAAE,SADM;AAEZjB,yBAAO,EAAE;AAFG,iBAAd;AAIAR,uBAAO,CAAC0B,GAAR,CAAY,uBAAZ,EAAqC;AACnCC,wBAAM,EAAE;AACNC,uBAAG,EAAEN,GAAG,CAACpB,IAAJ,CAAS2B;AADR;AAD2B,iBAArC,EAIGR,IAJH,CAIQS,IAAG,IAAK;AACdC,gCAAc,CAACC,OAAf,CAAuB,gBAAvB,EAAyC,QAAQF,IAAI,CAAC5B,IAAL,CAAU,CAAV,EAAa+B,cAArB,GAAsC,GAA/E;AACD,iBAND;AAOAF,8BAAc,CAACC,OAAf,CAAuB,WAAvB,EAAoCV,GAAG,CAACpB,IAAJ,CAASI,EAA7C;AACAyB,8BAAc,CAACC,OAAf,CAAuB,aAAvB,EAAsCV,GAAG,CAACpB,IAAJ,CAASD,IAA/C;AACA8B,8BAAc,CAACC,OAAf,CAAuB,YAAvB,EAAoCV,GAAG,CAACpB,IAAJ,CAAS2B,UAA7C;AACA,qBAAKK,OAAL,CAAaC,IAAb,CAAkB,OAAlB,EAfoB,CAeQ;AAC9B,eAhBA,MAgBO;AACL,qBAAKX,QAAL,CAAc;AACZC,sBAAI,EAAE,OADM;AAEZjB,yBAAO,EAAEc,GAAG,CAACc;AAFD,iBAAd;AAIF;AACD,aAvBD;AAwBF,WAzBA,MAyBO;AACL,iBAAKZ,QAAL,CAAc;AACZC,kBAAI,EAAE,OADM;AAEZjB,qBAAO,EAAE;AAFG,aAAd;AAIF;AACF;AACD,OAlCD;AAmCF;;AA3CO;AAlBI,CAAf,C,CAgEA;AACA;AACA;AACA;AACA","names":["Avatar","Lock","request","name","data","password2","form","rules","id","required","message","trigger","password","methods","test","console","log","login","$refs","validate","valid","length","post","then","res","code","$message","type","get","params","dep","department","res2","sessionStorage","setItem","departmentName","$router","push","msg"],"sourceRoot":"","sources":["E:\\code\\springboot+vue\\class_choose_book\\vue\\src\\views\\register.vue"],"sourcesContent":["<template>\r\n  <div class=\"index-bg\"></div>\r\n  <div  style=\"overflow: hidden;\">\r\n    <el-form ref=\"form\" :model=\"form\" class=\"login\" :rules=\"rules\"   >\r\n      <div style=\"color: darkslategrey; font-size: 30px; text-align: center; padding: 40px 0;\">登陆</div>\r\n      <el-form-item prop=\"id\">\r\n        <el-input :prefix-icon=\"Avatar\" placeholder=\"请输入ID/用户名\" v-model=\"form.id\"/>\r\n      </el-form-item>\r\n      <el-form-item prop=\"password\">\r\n        <el-input :prefix-icon=\"Lock\" placeholder=\"请输入密码\" v-model=\"form.password\" show-password/>\r\n      </el-form-item>\r\n      <el-form-item prop=\"password\">\r\n        <el-input :prefix-icon=\"Lock\" placeholder=\"请再次输入密码\" v-model=\"password2\" show-password @blur=\"test\"/>\r\n      </el-form-item>\r\n      <!--      <div style=\"text-align: center;\">-->\r\n      <!--        <el-button style=\"width: 80%;\" type=\"info\" @click=\"login\">登 录</el-button>-->\r\n      <!--        <div style=\"height: 50px; background-color: deepskyblue; width: 80%; margin: 0 auto;\"></div>-->\r\n      <!--      </div>-->\r\n      <el-button style=\"width: 100%;\" type=\"info\" @click=\"login\">登 录</el-button>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {Avatar, Lock} from '@element-plus/icons-vue';\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      Avatar,\r\n      Lock,\r\n      password2:'',\r\n      form: {},\r\n      rules: {\r\n        id: [\r\n          {required: true, message: '请输入用户名', trigger: 'blur'},\r\n        ],\r\n        password: [\r\n          {required: true, message: '请输入密码', trigger: 'blur'},\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    //判断两次密码是否一致\r\n    test(){\r\n      \r\n      console.log(this.form.password)\r\n      console.log(this.password2)\r\n    },\r\n    login() {\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          if (this.form.id.length == 8) { // 登录\r\n            request.post(\"/student/login\", this.form).then(res => {\r\n              if (res.code === '0') {\r\n                this.$message({\r\n                  type: \"success\",\r\n                  message: \"登录成功\"\r\n                })\r\n                request.get(\"/department/forHeader\", {\r\n                  params: {\r\n                    dep: res.data.department\r\n                  }\r\n                }).then(res2 => {\r\n                  sessionStorage.setItem(\"currentDepName\", \"你好，\" + res2.data[0].departmentName + \"，\")\r\n                })\r\n                sessionStorage.setItem(\"currentId\", res.data.id)\r\n                sessionStorage.setItem(\"currentName\", res.data.name)\r\n                sessionStorage.setItem(\"currentDep\",res.data.department)\r\n                this.$router.push(\"/home\"); // 登录成功之后进行页面跳转，跳转到主页\r\n              } else {\r\n                this.$message({\r\n                  type: \"error\",\r\n                  message: res.msg\r\n                })\r\n              }\r\n            })\r\n          } else {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: \"用户名不合法\"\r\n            })\r\n          }\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n// 注意，登录后sessionShort存储情况如下：\r\n// key=currentId,value=学号/工号\r\n// key=currentName,value=姓名\r\n// key=currentDep,value=院系号，管理员为0\r\n// key=currentDepName,value=\"你好，xxx学院，某某\"，管理员为空字符串\r\n</script>\r\n\r\n<style>\r\n.login {\r\n  background-color: wheat;\r\n  width: 30%;\r\n  height: 300px;\r\n  opacity: 0.8;\r\n  border-radius: 30px;\r\n  margin: 220px 300px 220px 500px;\r\n\r\n\r\n\r\n  /* 为其整体设置接近透明的效果*/\r\n  background-color: rgba( 255, 255, 255, 1);\r\n  /* 设置box-shadow使其有立体感 */\r\n  box-shadow: 5px 5px 0 0 rgba(0, 0, 0, 1);\r\n\r\n  padding: 0 50px;\r\n}\r\n*{\r\n  margin:0;\r\n}\r\n/*背景图*/\r\n.index-bg {\r\n  width: 100%;\r\n  height: 522px;\r\n  background: url(../assets/home-banner.jpg) no-repeat;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -1;\r\n  background-size: 100% 522px;\r\n  min-width: 1000px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}