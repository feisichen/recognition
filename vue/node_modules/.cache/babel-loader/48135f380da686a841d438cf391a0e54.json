{"ast":null,"code":"import Vue from 'vue';\nimport { Plus } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nimport { filesaver } from 'file-saver';\nimport uploader from 'vue-simple-uploader';\nimport App from './App.vue';\nVue.use(uploader);\nexport default {\n  name: \"recognition\",\n  components: {\n    Plus\n  },\n\n  data() {\n    return {\n      test_index: 0,\n      imageCounts: [],\n      classes: [],\n      newClass: '',\n      url: \"\",\n      path: '',\n      deskpath: '',\n      word: [],\n      number: 1,\n      downloadTxtButtonFlag: 0,\n      iftest: false,\n      ifsupport: false,\n      model: 'Conv4',\n      method: 'baseline++',\n      result: \"\",\n      loading: false\n    };\n  },\n\n  // created() {\n  //   window.addEventListener('unload', this.cleanup)\n  // },\n  // beforeDestroy() {\n  //   window.removeEventListener('unload', this.cleanup)\n  // },\n  methods: {\n    // downloadTxt() {\n    //   let str = this.word;\n    //   let strData = new Blob([str], { type: 'text/plain;charset=utf-8' });\n    //   saveAs(strData, \"翻译文件.txt\");\n    // },\n    addClass() {\n      console.log(this.newClass[0]);\n\n      if (this.newClass !== '' && this.newClass[0] !== '_') {\n        this.classes.push({\n          name: this.newClass\n        });\n        this.imageCounts.push(1);\n        this.newClass = '';\n      } else if (this.newClass === '') {\n        this.$message({\n          type: \"error\",\n          message: \"新类别名称不能为空！\"\n        });\n      } else if (this.newClass[0] === '_') {\n        this.$message({\n          type: \"error\",\n          message: \"新类别名称不能以'_'开头！\"\n        });\n      }\n    },\n\n    recognize() {\n      // this.result = \"\";\n      if (this.ifsupport && this.iftest) {\n        this.loading = true;\n        request.get(\"/file/recognize\", {\n          params: {\n            UserId: sessionStorage.getItem(\"currentId\"),\n            model: this.model,\n            method: this.method\n          }\n        }).then(res => {\n          this.result = res.data;\n          this.loading = false;\n        });\n      } else {\n        this.$message({\n          type: \"error\",\n          message: \"请提交支撑集和测试集\"\n        });\n      }\n    },\n\n    getImageIndex(index) {\n      // 返回当前类别已上传的图片数量\n      return this.imageCounts[index];\n    },\n\n    getTestImageIndex() {\n      // 返回当前类别已上传的图片数量\n      return this.test_index;\n    },\n\n    getUserId() {\n      // 返回当前类别已上传的图片数量\n      return sessionStorage.getItem(\"currentId\");\n    },\n\n    handleAvatarSuccess(res) {\n      this.ifsupport = true;\n      console.log(res);\n      this.imageCounts[res]++;\n    },\n\n    handleSuccess() {\n      this.test_index++;\n      this.iftest = true;\n    } // cleanup() {\n    //   request.get(\"/file/delete\", {\n    //     params: {\n    //       UserId:sessionStorage.getItem(\"currentId\")\n    //     }}).then(res => {\n    //     console.log(res)\n    //   })\n    // },\n\n\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,GAAP,MAAgB,KAAhB;AACA,SAAQC,IAAR,QAAmB,yBAAnB;AACA,OAAOC,OAAP,MAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,OAAOC,QAAP,MAAqB,qBAArB;AACA,OAAOC,GAAP,MAAgB,WAAhB;AACAL,GAAG,CAACM,GAAJ,CAAQF,QAAR;AAEA,eAAe;AACbG,EAAAA,IAAI,EAAE,aADO;AAEbC,EAAAA,UAAU,EAAE;AAACP,IAAAA;AAAD,GAFC;;AAGbQ,EAAAA,IAAI,GAAG;AACL,WAAO;AACLC,MAAAA,UAAU,EAAC,CADN;AAELC,MAAAA,WAAW,EAAE,EAFR;AAGLC,MAAAA,OAAO,EAAE,EAHJ;AAILC,MAAAA,QAAQ,EAAE,EAJL;AAKLC,MAAAA,GAAG,EAAE,EALA;AAMLC,MAAAA,IAAI,EAAC,EANA;AAOLC,MAAAA,QAAQ,EAAC,EAPJ;AAQLC,MAAAA,IAAI,EAAC,EARA;AASLC,MAAAA,MAAM,EAAC,CATF;AAULC,MAAAA,qBAAqB,EAAC,CAVjB;AAWLC,MAAAA,MAAM,EAAC,KAXF;AAYLC,MAAAA,SAAS,EAAC,KAZL;AAaLC,MAAAA,KAAK,EAAE,OAbF;AAcLC,MAAAA,MAAM,EAAE,YAdH;AAeLC,MAAAA,MAAM,EAAC,EAfF;AAgBLC,MAAAA,OAAO,EAAC;AAhBH,KAAP;AAkBD,GAtBY;;AAuBb;AACA;AACA;AACA;AACA;AACA;AACAC,EAAAA,OAAO,EAAC;AACN;AACA;AACA;AACA;AACA;AACAC,IAAAA,QAAQ,GAAG;AACTC,MAAAA,OAAO,CAACC,GAAR,CAAY,KAAKhB,QAAL,CAAc,CAAd,CAAZ;;AACA,UAAI,KAAKA,QAAL,KAAkB,