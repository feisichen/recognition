{"ast":null,"code":"import { Avatar, Lock } from '@element-plus/icons-vue';\nimport { Plus } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nexport default {\n  name: \"bookupload\",\n\n  data() {\n    return {\n      Avatar,\n      Lock,\n      form: {},\n      rules: {\n        id: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    beforeimgUpload(file) {\n      const fileName = file.name;\n      const fileType = fileName.substring(fileName.lastIndexOf('.')); // jpeg,.png,.jpg,.bmp,.gif\n\n      if (fileType === '.jpg' || fileType === '.png' || fileType === '.jpeg') {// 不处理\n      } else {\n        this.$message.error('不是,jpeg,.png,.jpg,.bmp,.gif文件,请上传正确的图片类型');\n        return false;\n      }\n    },\n\n    beforepdfUpload(file) {\n      const fileName = file.name;\n      const fileType = fileName.substring(fileName.lastIndexOf('.')); // pdf\n\n      if (fileType === '.pdf') {// 不处理\n      } else {\n        this.$message.error('不是.pdf文件,请上传正确的文件类型');\n        return false;\n      }\n    },\n\n    upload() {\n      if (sessionStorage.getItem(\"flag\") == 'false') {\n        if (this.form.bookname != null) {\n          this.form.uploader = sessionStorage.getItem(\"currentName\"), request.post(\"/book\", this.form).then(res => {\n            console.log(res);\n\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"上传成功\"\n              });\n              this.$router.replace('/book');\n            } else {\n              this.$message({\n                type: \"warning\",\n                message: res.msg\n              });\n            }\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: \"书名不合法\"\n          });\n        }\n      } else {\n        this.$message({\n          type: \"error\",\n          message: \"请先登陆\"\n        });\n      }\n    },\n\n    handleAvatarSuccess(res) {\n      this.form.imagepath = res;\n    },\n\n    pdfhandleAvatarSuccess(res) {\n      this.form.pdfpath = res;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AACA,SAAQA,MAAR,EAAgBC,IAAhB,QAA2B,yBAA3B;AACA,SAAQC,IAAR,QAAmB,yBAAnB;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAGA,eAAe;AACbC,EAAAA,IAAI,EAAE,YADO;;AAGbC,EAAAA,IAAI,GAAG;AAEL,WAAO;AAELL,MAAAA,MAFK;AAGLC,MAAAA,IAHK;AAILK,MAAAA,IAAI,EAAE,EAJD;AAKLC,MAAAA,KAAK,EAAE;AACLC,QAAAA,EAAE,EAAE,CACF;AAACC,UAAAA,QAAQ,EAAE,IAAX;AAAiBC,UAAAA,OAAO,EAAE,QAA1B;AAAoCC,UAAAA,OAAO,EAAE;AAA7C,SADE,CADC;AAILC,QAAAA,QAAQ,EAAE,CACR;AAACH,UAAAA,QAAQ,EAAE,IAAX;AAAiBC,UAAAA,OAAO,EAAE,OAA1B;AAAmCC,UAAAA,OAAO,EAAE;AAA5C,SADQ;AAJL;AALF,KAAP;AAcD,GAnBY;;AAoBbE,EAAAA,OAAO,EAAE;AACPC,IAAAA,eAAe,CAACC,IAAD,EAAO;AACpB,YAAMC,QAAQ,GAAGD,IAAI,CAACX,IAAtB;AACA,YAAMa,QAAQ,GAAGD,QAAQ,CAACE,SAAT,CAAmBF,QAAQ,CAACG,WAAT,CAAqB,GAArB,CAAnB,CAAjB,CAFoB,CAGpB;;AACA,UACIF,QAAQ,KAAK,MAAb,IACAA,QAAQ,KAAK,MADb,IAEAA,QAAQ,KAAK,OAHjB,EAIE,CACA;AACD,OAND,MAMO;AACL,aAAKG,QAAL,CAAcC,KAAd,CACI,0CADJ;AAGA,eAAO,KAAP;AACD;AACF,KAjBM;;AAkBPC,IAAAA,eAAe,CAACP,IAAD,EAAO;AACpB,YAAMC,QAAQ,GAAGD,IAAI,CAACX,IAAtB;AACA,YAAMa,QAAQ,GAAGD,QAAQ,CAACE,SAAT,CAAmBF,QAAQ,CAACG,WAAT,CAAqB,GAArB,CAAnB,CAAjB,CAFoB,CAGpB;;AACA,UACIF,QAAQ,KAAK,MADjB,EAGE,CACA;AACD,OALD,MAKO;AACL,aAAKG,QAAL,CAAcC,KAAd,CACI,qBADJ;AAGA,eAAO,KAAP;AACD;AACF,KAjCM;;AAkCPE,IAAAA,MAAM,GAAG;AACP,UAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,OAAtC,EAA+C;AAC7C,YAAI,KAAKnB,IAAL,CAAUoB,QAAV,IAAsB,IAA1B,EAAgC;AAC9B,eAAKpB,IAAL,CAAUqB,QAAV,GAAmBH,cAAc,CAACC,OAAf,CAAuB,aAAvB,CAAnB,EACAtB,OAAO,CAACyB,IAAR,CAAa,OAAb,EAAsB,KAAKtB,IAA3B,EAAiCuB,IAAjC,CAAsCC,GAAG,IAAI;AAC3CC,YAAAA,OAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,gBAAIA,GAAG,CAACG,IAAJ,KAAa,GAAjB,EAAsB;AACpB,mBAAKb,QAAL,CAAc;AACZc,gBAAAA,IAAI,EAAE,SADM;AAEZxB,gBAAAA,OAAO,EAAE;AAFG,eAAd;AAIA,mBAAKyB,OAAL,CAAaC,OAAb,CAAqB,OAArB;AACD,aAND,MAMO;AACL,mBAAKhB,QAAL,CAAc;AACZc,gBAAAA,IAAI,EAAE,SADM;AAEZxB,gBAAAA,OAAO,EAAEoB,GAAG,CAACO;AAFD,eAAd;AAID;AACF,WAdD,CADA;AAgBD,SAjBD,MAiBO;AACL,eAAKjB,QAAL,CAAc;AACZc,YAAAA,IAAI,EAAE,OADM;AAEZxB,YAAAA,OAAO,EAAE;AAFG,WAAd;AAID;AACF,OAxBD,MAwBO;AACL,aAAKU,QAAL,CAAc;AACZc,UAAAA,IAAI,EAAE,OADM;AAEZxB,UAAAA,OAAO,EAAE;AAFG,SAAd;AAID;AACF,KAjEM;;AAkEP4B,IAAAA,mBAAmB,CAACR,GAAD,EAAM;AACvB,WAAKxB,IAAL,CAAUiC,SAAV,GAAsBT,GAAtB;AACD,KApEM;;AAqEPU,IAAAA,sBAAsB,CAACV,GAAD,EAAM;AAC1B,WAAKxB,IAAL,CAAUmC,OAAV,GAAoBX,GAApB;AACD;;AAvEM;AApBI,CAAf","names":["Avatar","Lock","Plus","request","name","data","form","rules","id","required","message","trigger","password","methods","beforeimgUpload","file","fileName","fileType","substring","lastIndexOf","$message","error","beforepdfUpload","upload","sessionStorage","getItem","bookname","uploader","post","then","res","console","log","code","type","$router","replace","msg","handleAvatarSuccess","imagepath","pdfhandleAvatarSuccess","pdfpath"],"sourceRoot":"","sources":["E:\\code\\springboot+vue\\class_choose_book1\\vue\\src\\views\\Bookupload.vue"],"sourcesContent":["<template>\r\n  <div class=\"bg\"></div>\r\n\r\n  <el-card class=\"loader\">\r\n    <div class=\"edit\">\r\n      <div class=\"edit-item\">\r\n        <span class=\"label\">上传封面</span>\r\n        <el-upload\r\n            class=\"avatar-uploader\"\r\n            action=\"http://localhost:9090/file/upload\"\r\n            :show-file-list=\"false\"\r\n            :on-success=\"handleAvatarSuccess\"\r\n            accept=\".jpeg,.png,.jpg\"\r\n            :before-upload=\"beforeimgUpload\"\r\n        >\r\n          <img v-if=\"form.imagepath\" :src=\"form.imagepath\" class=\"avatar\"/>\r\n          <el-icon v-else class=\"avatar-uploader-icon\">\r\n            <Plus/>\r\n          </el-icon>\r\n        </el-upload>\r\n      </div>\r\n      <div class=\"edit-item\">\r\n        <span class=\"label\">ID</span>\r\n        <div>\r\n          <!--          //用v-model来绑定数据-->\r\n          <el-input v-model=\"form.id\" class=\"create-input\" placeholder=\"请输入内容\"></el-input>\r\n        </div>\r\n      </div>\r\n      <div class=\"edit-item\">\r\n        <span class=\"label\">书名</span>\r\n        <div>\r\n          <!--          //用v-model来绑定数据-->\r\n          <el-input v-model=\"form.bookname\" class=\"create-input\" placeholder=\"请输入内容\"></el-input>\r\n        </div>\r\n      </div>\r\n<!--      <div class=\"edit-item\">-->\r\n<!--        <span class=\"label\">上传者</span>-->\r\n<!--        <div>-->\r\n<!--          &lt;!&ndash;          //用v-model来绑定数据&ndash;&gt;-->\r\n<!--          <el-input v-model=\"form.uploader\" class=\"create-input\" placeholder=\"请输入内容\"></el-input>-->\r\n<!--        </div>-->\r\n<!--      </div>-->\r\n      <!--      <div class=\"edit-item\">-->\r\n      <!--        <span class=\"label\">个人简介</span>-->\r\n      <!--        <div>-->\r\n      <!--          <el-input  v-model=\"sign\"  type=\"textarea\" class=\"create-input\" placeholder=\"请输入内容\"></el-input>-->\r\n      <!--        </div>-->\r\n      <!--      </div>-->\r\n      <el-upload\r\n          v-model:file-list=\"fileList\"\r\n          class=\"upload-demo\"\r\n          action=\"http://localhost:9090/file/uploadpdf\"\r\n          accept=\".pdf\"\r\n          :before-upload=\"beforepdfUpload\"\r\n          :on-success=\"pdfhandleAvatarSuccess\"\r\n          :limit=\"1\"\r\n      >\r\n        <el-button type=\"primary\">Click to upload</el-button>\r\n        <template #tip>\r\n          <div class=\"el-upload__tip\">\r\n            pdf files with a size less than 2MB and just one.\r\n          </div>\r\n        </template>\r\n      </el-upload>\r\n      <div>\r\n        <el-button class=\"send\" type=\"primary\" size=\"medium\" @click=\"upload\"\r\n        >上传\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n  </el-card>\r\n</template>\r\n\r\n<script >\r\nimport {Avatar, Lock} from '@element-plus/icons-vue';\r\nimport {Plus} from '@element-plus/icons-vue';\r\nimport request from \"@/utils/request\";\r\n\r\n\r\nexport default {\r\n  name: \"bookupload\",\r\n\r\n  data() {\r\n\r\n    return {\r\n\r\n      Avatar,\r\n      Lock,\r\n      form: {},\r\n      rules: {\r\n        id: [\r\n          {required: true, message: '请输入用户名', trigger: 'blur'},\r\n        ],\r\n        password: [\r\n          {required: true, message: '请输入密码', trigger: 'blur'},\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    beforeimgUpload(file) {\r\n      const fileName = file.name\r\n      const fileType = fileName.substring(fileName.lastIndexOf('.'))\r\n      // jpeg,.png,.jpg,.bmp,.gif\r\n      if (\r\n          fileType === '.jpg' ||\r\n          fileType === '.png' ||\r\n          fileType === '.jpeg'\r\n      ) {\r\n        // 不处理\r\n      } else {\r\n        this.$message.error(\r\n            '不是,jpeg,.png,.jpg,.bmp,.gif文件,请上传正确的图片类型'\r\n        )\r\n        return false\r\n      }\r\n    },\r\n    beforepdfUpload(file) {\r\n      const fileName = file.name\r\n      const fileType = fileName.substring(fileName.lastIndexOf('.'))\r\n      // pdf\r\n      if (\r\n          fileType === '.pdf'\r\n\r\n      ) {\r\n        // 不处理\r\n      } else {\r\n        this.$message.error(\r\n            '不是.pdf文件,请上传正确的文件类型'\r\n        )\r\n        return false\r\n      }\r\n    },\r\n    upload() {\r\n      if (sessionStorage.getItem(\"flag\") == 'false') {\r\n        if (this.form.bookname != null) {\r\n          this.form.uploader=sessionStorage.getItem(\"currentName\"),\r\n          request.post(\"/book\", this.form).then(res => {\r\n            console.log(res);\r\n            if (res.code === '0') {\r\n              this.$message({\r\n                type: \"success\",\r\n                message: \"上传成功\"\r\n              })\r\n              this.$router.replace('/book')\r\n            } else {\r\n              this.$message({\r\n                type: \"warning\",\r\n                message: res.msg\r\n              })\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: \"书名不合法\"\r\n          })\r\n        }\r\n      } else {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"请先登陆\"\r\n        })\r\n      }\r\n    },\r\n    handleAvatarSuccess(res) {\r\n      this.form.imagepath = res;\r\n    },\r\n    pdfhandleAvatarSuccess(res) {\r\n      this.form.pdfpath = res;\r\n    },\r\n\r\n\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.edit {\r\n  padding: 80px 0 20px 20px\r\n}\r\n\r\n.edit-item {\r\n  display: flex;\r\n  margin-bottom: 20px\r\n}\r\n\r\n.label {\r\n  margin-right: 10px\r\n}\r\n\r\n.avatar-uploader .avatar {\r\n  width: 178px;\r\n  height: 228px;\r\n  display: block;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed var(--el-border-color);\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n  transition: var(--el-transition-duration-fast);\r\n}\r\n\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: var(--el-color-primary);\r\n}\r\n\r\n.el-icon.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  text-align: center;\r\n}\r\n\r\n.loader {\r\n  background-color: wheat;\r\n  width: 50%;\r\n  height: 750px;\r\n  opacity: 0.8;\r\n  border-radius: 30px;\r\n  margin: 100px 0 220px 360px;\r\n\r\n\r\n  /* 为其整体设置接近透明的效果*/\r\n  background-color: #ffffff;\r\n  /* 设置box-shadow使其有立体感 */\r\n  /*box-shadow: 5px 5px 0 0 rgba(0, 0, 0, 1);*/\r\n\r\n  padding: 0 50px;\r\n}\r\n\r\n* {\r\n  margin: 0;\r\n}\r\n\r\n/*背景图*/\r\n.bg {\r\n  width: 100%;\r\n  height: 762px;\r\n  background-color: #ffffff;\r\n  position: absolute;\r\n  margin-top: 58px;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -1;\r\n  background-size: 100% 762px;\r\n  min-width: 1000px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}