{"ast":null,"code":"import request from \"@/utils/request\"; // import java.util.Base64;\n\nexport default {\n  name: \"book\",\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      search: '',\n      tableData: [],\n      currentPage: 1,\n      pageSize: 12,\n      total: 0\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/book\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search // selectDep: this.selectDep\n\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        console.log(this.tableData);\n        this.total = res.data.total;\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(currentPage) {\n      this.currentPage = currentPage;\n      this.load();\n    },\n\n    goToread(id) {\n      sessionStorage.setItem(\"id\", id);\n      console.log(id); // 跳转路由\n\n      this.$router.push(\"/read\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,OAAP,MAAoB,iBAApB,C,CACA;;AAEA,eAAe;AACbC,EAAAA,IAAI,EAAC,MADQ;AAEbC,EAAAA,UAAU,EAAC,EAFE;;AAKbC,EAAAA,IAAI,GAAE;AACJ,WAAM;AACJC,MAAAA,IAAI,EAAE,EADF;AAEJC,MAAAA,MAAM,EAAC,EAFH;AAGJC,MAAAA,SAAS,EAAE,EAHP;AAIJC,MAAAA,WAAW,EAAE,CAJT;AAKJC,MAAAA,QAAQ,EAAE,EALN;AAMJC,MAAAA,KAAK,EAAE;AANH,KAAN;AAQD,GAdY;;AAebC,EAAAA,OAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAjBY;;AAkBbC,EAAAA,OAAO,EAAC;AACND,IAAAA,IAAI,GAAG;AACLX,MAAAA,OAAO,CAACa,GAAR,CAAY,OAAZ,EAAqB;AACnBC,QAAAA,MAAM,EAAE;AACNC,UAAAA,OAAO,EAAE,KAAKR,WADR;AAENC,UAAAA,QAAQ,EAAE,KAAKA,QAFT;AAGNH,UAAAA,MAAM,EAAE,KAAKA,MAHP,CAIN;;AAJM;AADW,OAArB,EAOGW,IAPH,CAOQC,GAAG,IAAI;AACbC,QAAAA,OAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKX,SAAL,GAAiBW,GAAG,CAACd,IAAJ,CAASiB,OAA1B;AACAF,QAAAA,OAAO,CAACC,GAAR,CAAY,KAAKb,SAAjB;AACA,aAAKG,KAAL,GAAaQ,GAAG,CAACd,IAAJ,CAASM,KAAtB;AACD,OAZD;AAaD,KAfK;;AAgBNY,IAAAA,