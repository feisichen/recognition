le","log","handleSuccess"],"sourceRoot":"","sources":["D:\\code\\Java\\image_recognition\\vue\\src\\views\\Recognition.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <input type=\"text\" v-model=\"newClass\">\r\n    <button @click=\"addClass\">Add Class</button>\r\n    <hr>\r\n    <div v-for=\"(item, index) in classes\" :key=\"index\">\r\n      <h3>{{ item.name }}</h3>\r\n      <el-upload\r\n          class=\"upload-demo\"\r\n          drag\r\n          action=\"http://localhost:9090/file/upload\"\r\n          :on-success=\"handleAvatarSuccess\"\r\n          list-type=\"picture\"\r\n          multiple\r\n          :data=\"{ Index:index, ClassName: item.name, UserId:getUserId(), ImageIndex: getImageIndex(index) }\"\r\n      >\r\n        <el-icon class=\"el-icon--upload\"><Plus/></el-icon>\r\n        <div class=\"el-upload__text\">\r\n          拖到这里 /<em>点击上传</em>\r\n        </div>\r\n        <template #tip>\r\n          <div class=\"el-upload__tip\">\r\n            jpg/png files with a size less than 500kb\r\n          </div>\r\n        </template>\r\n      </el-upload>\r\n    </div>\r\n  </div>\r\n  <el-upload\r\n      class=\"upload-demo\"\r\n      drag\r\n      action=\"http://localhost:9090/file/upload_test\"\r\n      :on-ssuccess=\"handleSuccess\"\r\n      list-type=\"picture\"\r\n      multiple\r\n      :data=\"{ UserId:getUserId(), ImageIndex: testCounts }\"\r\n  >\r\n    <el-icon class=\"el-icon--upload\"><Plus/></el-icon>\r\n    <div class=\"el-upload__text\">\r\n      拖到这里 /<em>点击上传</em>\r\n    </div>\r\n    <template #tip>\r\n      <div class=\"el-upload__tip\">\r\n        jpg/png files with a size less than 500kb\r\n      </div>\r\n    </template>\r\n  </el-upload>\r\n  <div class=\"bg\"></div>\r\n  <el-button @click=\"recognize\" color=\"#ffd04b\" style=\"margin:13px 10px; height: auto; border-radius:30px;\">识别</el-button>\r\n  <div>\r\n  <select v-model=\"model\">\r\n    <option disabled value=\"\">Please select an option</option>\r\n    <option>Conv4</option>\r\n    <option>Conv6</option>\r\n    <option>ResNet10</option>\r\n    <option>ResNet101</option>\r\n  </select>\r\n\r\n  <select v-model=\"method\">\r\n    <option disabled value=\"\">Please select an option</option>\r\n    <option>baseline</option>\r\n    <option>baseline++</option>\r\n  </select>\r\n\r\n  <div>Selected option1: {{ model }}</div>\r\n  <div>Selected option2: {{ method }}</div>\r\n  </div>\r\n  <div>\r\n    <p>识别结果：{{ result }}</p>\r\n  </div>\r\n  <div>\r\n    <p>识别结果：{{ imageCounts }}</p>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport {Plus} from '@element-plus/icons-vue';\r\nimport request from \"@/utils/request\";\r\nimport { filesaver } from 'file-saver';\r\n\r\nexport default {\r\n  name: \"recognition\",\r\n  components: {Plus},\r\n  data() {\r\n    return {\r\n      imageCounts: [],\r\n      classes: [],\r\n      newClass: '',\r\n      url: \"\",\r\n      path:'',\r\n      deskpath:'',\r\n      word:[],\r\n      number:1,\r\n      downloadTxtButtonFlag:0,\r\n      testCounts:1,\r\n      model: 'Conv4',\r\n      method: 'baseline++',\r\n      result:\"\"\r\n    }\r\n  },\r\n  // created() {\r\n  //   window.addEventListener('unload', this.cleanup)\r\n  // },\r\n  // beforeDestroy() {\r\n  //   window.removeEventListener('unload', this.cleanup)\r\n  // },\r\n  methods:{\r\n    // downloadTxt() {\r\n    //   let str = this.word;\r\n    //   let strData = new Blob([str], { type: 'text/plain;charset=utf-8' });\r\n    //   saveAs(strData, \"翻译文件.txt\");\r\n    // },\r\n    addClass() {\r\n      if (this.newClass !== '') {\r\n        this.classes.push({\r\n          name: this.newClass\r\n        });\r\n        this.imageCounts.push(1);\r\n        this.newClass = '';\r\n      }\r\n    },\r\n\r\n    recognize() {\r\n      request.get(\"/file/recognize\", {\r\n            params: {\r\n              UserId:sessionStorage.getItem(\"currentId\"),\r\n              model:this.model,\r\n              method:this.method\r\n            }}).then(res => {\r\n              this.result = res;\r\n      })\r\n    },\r\n    getImageIndex(index) {\r\n      // 返回当前类别已上传的图片数量\r\n      return this.imageCounts[index]\r\n    },\r\n    getUserId() {\r\n      // 返回当前类别已上传的图片数量\r\n      return sessionStorage.getItem(\"currentId\")\r\n    },\r\n    handleAvatarSuccess(res)\r\n    {\r\n      console.log(res)\r\n      this.imageCounts[res]++\r\n    },\r\n    handleSuccess()\r\n    {\r\n      testCounts++\r\n    },\r\n    // cleanup() {\r\n    //   request.get(\"/file/delete\", {\r\n    //     params: {\r\n    //       UserId:sessionStorage.getItem(\"currentId\")\r\n    //     }}).then(res => {\r\n    //     console.log(res)\r\n    //   })\r\n    // },\r\n  }\r\n}\r\n</script>\r\n<style>\r\n.upcard{\r\n  align-content: center;\r\n  background-color: wheat;\r\n  width: 85%;\r\n  min-height: 550px;\r\n  height: 100%;\r\n  opacity: 0.8;\r\n  border-radius: 30px;\r\n  margin: 50px 0 60px 60px;\r\n\r\n\r\n  /* 为其整体设置接近透明的效果*/\r\n  background-color: #ffffff;\r\n  /* 设置box-shadow使其有立体感 */\r\n  /*box-shadow: 5px 5px 0 0 rgba(0, 0, 0, 1);*/\r\n\r\n  padding: 0 50px;\r\n}\r\n.bg {\r\n  width: 100%;\r\n  height: 580px;\r\n  background-color: #ffffff;\r\n  position: absolute;\r\n  margin-top: 58px;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -1;\r\n  background-size: 100% 522px;\r\n  min-width: 1000px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}