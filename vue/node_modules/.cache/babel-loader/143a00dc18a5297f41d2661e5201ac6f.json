{"ast":null,"code":"import { Plus } from '@element-plus/icons-vue';\nimport request from \"@/utils/request\";\nimport { filesaver } from 'file-saver';\nexport default {\n  name: \"recognition\",\n  components: {\n    Plus\n  },\n\n  data() {\n    return {\n      imageCounts: [],\n      classes: [],\n      newClass: '',\n      url: \"\",\n      path: '',\n      deskpath: '',\n      word: [],\n      number: 1,\n      downloadTxtButtonFlag: 0,\n      testCounts: 1,\n      model: 'Conv4',\n      method: 'baseline++',\n      result: \"\"\n    };\n  },\n\n  // created() {\n  //   window.addEventListener('unload', this.cleanup)\n  // },\n  // beforeDestroy() {\n  //   window.removeEventListener('unload', this.cleanup)\n  // },\n  methods: {\n    // downloadTxt() {\n    //   let str = this.word;\n    //   let strData = new Blob([str], { type: 'text/plain;charset=utf-8' });\n    //   saveAs(strData, \"翻译文件.txt\");\n    // },\n    addClass() {\n      if (this.newClass !== '') {\n        this.classes.push({\n          name: this.newClass\n        });\n        this.imageCounts.push(1);\n        this.newClass = '';\n      }\n    },\n\n    recognize() {\n      request.get(\"/file/recognize\", {\n        params: {\n          UserId: sessionStorage.getItem(\"currentId\"),\n          model: this.model,\n          method: this.method\n        }\n      }).then(res => {\n        this.result = res;\n      });\n    },\n\n    getImageIndex(index) {\n      // 返回当前类别已上传的图片数量\n      return this.imageCounts[index];\n    },\n\n    getUserId() {\n      // 返回当前类别已上传的图片数量\n      return sessionStorage.getItem(\"currentId\");\n    },\n\n    handleAvatarSuccess(res) {\n      console.log(res);\n      this.imageCounts[res]++;\n    },\n\n    handleSuccess() {\n      testCounts++;\n    } // cleanup() {\n    //   request.get(\"/file/delete\", {\n    //     params: {\n    //       UserId:sessionStorage.getItem(\"currentId\")\n    //     }}).then(res => {\n    //     console.log(res)\n    //   })\n    // }