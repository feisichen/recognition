{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"ManageDepartment\",\n\n  data() {\n    return {\n      tableData: [],\n      dialogVisible: false,\n      dialogVisible2: false,\n      form: {},\n      form2: {},\n      total: 0\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/department/getInfo\", {\n        params: {\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = this.tableData.length;\n      });\n    },\n\n    goToCheckStudent(row) {\n      // 缓存当前课程信息\n      sessionStorage.setItem(\"currentCheckDepartment\", row.department);\n      sessionStorage.setItem(\"currentCheckDepartmentName\", row.departmentName); // 跳转路由\n\n      this.$router.push(\"/manageDepStudent\");\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    handleEdit(row) {\n      this.form2 = JSON.parse(JSON.stringify(row));\n      this.dialogVisible2 = true;\n    },\n\n    handleDelete(department) {\n      request.delete(\"/department/\" + department).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    save() {\n      request.post(\"/department\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"warning\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    update() {\n      request.put(\"/department\", this.form2).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible2 = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmEA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE,EADN;AAELC,mBAAa,EAAE,KAFV;AAGLC,oBAAc,EAAE,KAHX;AAILC,UAAI,EAAE,EAJD;AAKLC,WAAK,EAAE,EALF;AAMLC,WAAK,EAAE;AANF,KAAP;AAQD,GAXY;;AAYbC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAdY;;AAebC,SAAO,EAAE;AACPD,QAAI,GAAG;AACLV,aAAO,CAACY,GAAR,CAAY,qBAAZ,EAAmC;AACjCC,cAAM,EAAE;AACNC,gBAAM,EAAE,KAAKA;AADP;AADyB,OAAnC,EAIGC,IAJH,CAIQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKb,SAAL,GAAiBa,GAAG,CAACd,IAArB;AACA,aAAKM,KAAL,GAAa,KAAKL,SAAL,CAAegB,MAA5B;AACD,OARD;AASD,KAXM;;AAYPC,oBAAgB,CAACC,GAAD,EAAM;AACpB;AACAC,oBAAc,CAACC,OAAf,CAAuB,wBAAvB,EAAiDF,GAAG,CAACG,UAArD;AACAF,oBAAc,CAACC,OAAf,CAAuB,4BAAvB,EAAqDF,GAAG,CAACI,cAAzD,EAHoB,CAIpB;;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB,mBAAlB;AACD,KAlBM;;AAmBPC,OAAG,GAAG;AACJ,WAAKxB,aAAL,GAAqB,IAArB;AACA,WAAKE,IAAL,GAAY,EAAZ;AACD,KAtBM;;AAuBPuB,cAAU,CAACR,GAAD,EAAM;AACd,WAAKd,KAAL,GAAauB,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeX,GAAf,CAAX,CAAb;AACA,WAAKhB,cAAL,GAAsB,IAAtB;AACD,KA1BM;;AA2BP4B,gBAAY,CAACT,UAAD,EAAa;AACvBxB,aAAO,CAACkC,MAAR,CAAe,iBAAiBV,UAAhC,EAA4CT,IAA5C,CAAiDC,GAAE,IAAK;AACtD,YAAIA,GAAG,CAACmB,IAAJ,KAAa,GAAjB,EAAsB;AACpB,eAAKC,QAAL,CAAc;AACZC,gBAAI,EAAE,SADM;AAEZC,mBAAO,EAAE;AAFG,WAAd;AAIA,eAAK5B,IAAL;AACF,SANA,MAMO;AACL,eAAK0B,QAAL,CAAc;AACZC,gBAAI,EAAE,OADM;AAEZC,mBAAO,EAAEtB,GAAG,CAACuB;AAFD,WAAd;AAIF;AACD,OAbD;AAcD,KA1CM;;AA2CPC,QAAI,GAAG;AACLxC,aAAO,CAACyC,IAAR,CAAa,aAAb,EAA4B,KAAKnC,IAAjC,EAAuCS,IAAvC,CAA4CC,GAAE,IAAK;AACjDC,eAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,YAAI