{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  data() {\n    return {\n      dataForm: {\n        UserName: '',\n        Telephone: '',\n        Institution: '',\n        Sex: '',\n        Address: '',\n        Info: ''\n      }\n    };\n  },\n\n  created() {\n    request.get(\"/user/get\", {\n      params: {\n        UserId: sessionStorage.getItem(\"currentId\")\n      }\n    }).then(res => {\n      this.dataForm.UserName = res.data.name;\n      this.dataForm.Telephone = res.data.telephone;\n      this.dataForm.Institution = res.data.institution;\n      this.dataForm.Sex = res.data.sex;\n      this.dataForm.Address = res.data.address;\n      this.dataForm.Info = res.data.info;\n    });\n  },\n\n  methods: {\n    back() {\n      // if (window.history.length <= 1) {\n      //   this.$router.push({path:'/'})\n      //   return false\n      // } else {\n      //   this.$router.go(-1)\n      // }\n      this.$router.push({\n        path: '/person'\n      });\n    },\n\n    commit() {\n      request.get(\"/user/commit\", {\n        params: {\n          id: sessionStorage.getItem(\"currentId\"),\n          telephone: this.dataForm.Telephone,\n          institution: this.dataForm.Institution,\n          sex: this.dataForm.Sex,\n          address: this.dataForm.Address,\n          info: this.dataForm.Info\n        }\n      }).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"修改成功\"\n          });\n        }\n      });\n      this.$router.push({\n        path: '/person'\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,EAAAA,IAAI,GAAE;AACJ,WAAM;AACJC,MAAAA,QAAQ,EAAC;AACPC,QAAAA,QAAQ,EAAE,EADH;AAEPC,QAAAA,SAAS,EAAE,EAFJ;AAGPC,QAAAA,WAAW,EAAC,EAHL;AAIPC,QAAAA,GAAG,EAAC,EAJG;AAKPC,QAAAA,OAAO,EAAC,EALD;AAMPC,QAAAA,IAAI,EAAE;AANC;AADL,KAAN;AAUD,GAZY;;AAabC,EAAAA,OAAO,GAAE;AACPT,IAAAA,OAAO,CAACU,GAAR,CAAY,WAAZ,EA